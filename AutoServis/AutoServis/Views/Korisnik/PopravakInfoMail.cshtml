@model AutoServis.ViewModels.RepairInfoViewModel

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
@Html.Partial("_PopravakInfo", Model)
<address>
    @Model.Kontakt.Adresa <br/>
    @Model.Kontakt.Mjesto <br/>
    Telefon : @Model.Kontakt.BrojTel
</address>
<address>
    <strong>Email:</strong> <a href="@Model.Kontakt.Email">@Model.Kontakt.Email</a><br/>
</address>
@Scripts.Render("~/bundles/bootstrap")
</body>
</html>